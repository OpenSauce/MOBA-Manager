<Window x:Name="MOBA_Manager" x:Class="MOBA_Manager.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MOBA_Manager"
        mc:Ignorable="d"
        Title="MOBA-Manager" Height="450" Width="800">
    <Grid>
        <Button x:Name="userNextButton" Content="Next" Height="26" Margin="0,356,100,0" VerticalAlignment="Top" Width="84" Click="userNextButton_Click" HorizontalAlignment="Right" IsEnabled="False"/>
        <TextBox x:Name="NameField" HorizontalAlignment="Center" Margin="0,65,0,0" TextWrapping="Wrap" VerticalAlignment="Top" RenderTransformOrigin="-0.11,5.066" Width="250" TextChanged="NameField_TextChanged"/>
        <Label Content="Name:" HorizontalAlignment="Center" Height="26" Margin="0,35,0,0" VerticalAlignment="Top"/>
        <Label Content="Age:" HorizontalAlignment="Center" Margin="0,95,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="AgeField" HorizontalAlignment="Center" Height="22" Margin="0,125,0,0" TextWrapping="Wrap" Text="99" VerticalAlignment="Top"/>

    </Grid>
</Window>
