<Window x:Name="Settings" x:Class="MOBA_Manager.SettingsPage"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MOBA_Manager"
        mc:Ignorable="d"
        Title="SettingsPage" Height="450" Width="800">
    <Grid Background="#FF460303">
        <Label Content="Resolution:" HorizontalAlignment="Left" Margin="123,59,0,0" VerticalAlignment="Top" Background="#00000000" Foreground="White" FontFamily="Segoe UI Light" Width="82" FontSize="14"/>
        <Label Content="Full Screen:" HorizontalAlignment="Left" Margin="123,93,0,0" VerticalAlignment="Top" Background="#00000000" Foreground="White" FontFamily="Segoe UI Light" Width="82" FontSize="14"/>
        <CheckBox Content="CheckBox" HorizontalAlignment="Left" Margin="223,101,0,0" VerticalAlignment="Top" Foreground="White"/>
        <ComboBox HorizontalAlignment="Left" Margin="223,66,0,0" VerticalAlignment="Top" Width="120"/>
        <Button Content="Apply" HorizontalAlignment="Left" Margin="323,369,0,0" VerticalAlignment="Top" Width="75" Click="Button_Click_1"/>
        <Button Content="Exit" HorizontalAlignment="Left" Margin="427,369,0,0" VerticalAlignment="Top" Width="75" Click="Button_Click"/>

    </Grid>
</Window>
