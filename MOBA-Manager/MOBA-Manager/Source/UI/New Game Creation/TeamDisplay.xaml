<UserControl x:Class="MOBA_Manager.Source.UI.New_Game_Creation.TeamDisplay"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:local="clr-namespace:MOBA_Manager.Source.UI.New_Game_Creation"
             mc:Ignorable="d"
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        <Label x:Name="TeamName" Content="Label" HorizontalAlignment="Left" Margin="200,50,0,0" VerticalAlignment="Top" Style="{DynamicResource Header}" />
        <Image x:Name="TeamImage" HorizontalAlignment="Left" Height="100" Margin="75,29,0,0" VerticalAlignment="Top" Width="100" />
    </Grid>
</UserControl>