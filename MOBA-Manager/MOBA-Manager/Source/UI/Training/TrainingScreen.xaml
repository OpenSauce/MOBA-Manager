<Page x:Class="MOBA_Manager.Source.UI.Training.TrainingScreen"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
      xmlns:local="clr-namespace:MOBA_Manager.Source.UI.Training"
      mc:Ignorable="d"
      d:DesignHeight="450" d:DesignWidth="800"
      Title="TrainingScreen">

    <Grid>
        <Label Content="Label" HorizontalAlignment="Center" Margin="0,40,0,0" VerticalAlignment="Top" Style="{DynamicResource TitleLabel}" />
        <WrapPanel x:Name="TrainingPlayers" Margin="0,150,0,0" />
    </Grid>
</Page>