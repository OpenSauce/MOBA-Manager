<Page x:Class="MOBA_Manager.UI.IntroScreen"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
      xmlns:local="clr-namespace:MOBA_Manager.UI"
      mc:Ignorable="d"
      d:DesignHeight="450" d:DesignWidth="800"
      Title="IntoScreen">

    <Grid Background="#FF1B3B91">
        <Button x:Name="BeginButton" Content="Begin" HorizontalAlignment="Center" Margin="0" VerticalAlignment="Center" Width="75" Click="Button_Click" BorderBrush="{x:Null}" Foreground="White" Background="{x:Null}" FontSize="24" MouseEnter="Button_MouseEnter" MouseLeave="BeginButton_MouseLeave" />
        <Button Content="Load" HorizontalAlignment="Center" Margin="362,100,362,25" VerticalAlignment="Center" Width="76" Click="Button_Click" BorderBrush="#331B3B91" Foreground="White" Background="{x:Null}" FontSize="18" />
        <Image HorizontalAlignment="Center" Margin="0,100,0,0" VerticalAlignment="Top" Width="431" Source="/MOBA-Manager;component/Assets/SplashScreen/MOBA-Black.png"/>
        <Image HorizontalAlignment="Center" Margin="0,200,0,0" VerticalAlignment="Top" Width="767" Source="/MOBA-Manager;component/Assets/SplashScreen/Manager-Black.png"/>
    </Grid>
</Page>