<Page x:Class="MOBA_Manager.Source.UI.InGame.Fixtures.FixturesPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
      xmlns:local="clr-namespace:MOBA_Manager.Source.UI.InGame.Fixtures"
      mc:Ignorable="d"
      d:DesignHeight="450" d:DesignWidth="800"
      Title="FixturesPage">

    <Grid>
        <ScrollViewer VerticalScrollBarVisibility="Auto" Margin="100">
            <StackPanel x:Name="BuyPlayerStackPanel" Margin="0">
                <StackPanel.Resources>
                    <Style TargetType="{x:Type UserControl}">
                        <Setter Property="Margin" Value="10,10,10,10" />
                    </Style>
                </StackPanel.Resources>
                <StackPanel.Background>
                    <SolidColorBrush Color="#FF191B1B" Opacity="0.6" />
                </StackPanel.Background>
            </StackPanel>
        </ScrollViewer>
    </Grid>
</Page>