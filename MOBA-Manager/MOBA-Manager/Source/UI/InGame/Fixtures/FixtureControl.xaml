<UserControl x:Class="MOBA_Manager.Source.UI.InGame.Fixtures.FixtureControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:local="clr-namespace:MOBA_Manager.Source.UI.InGame.Fixtures"
             mc:Ignorable="d"
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid Height="50">
        <Label x:Name="HomeTeam" Content="Label" HorizontalAlignment="Left" Margin="250,0,0,0" Style="{DynamicResource Header}" VerticalAlignment="Center" VerticalContentAlignment="Center" HorizontalContentAlignment="Center" />
        <Label x:Name="AwayTeam" Content="Label" HorizontalAlignment="Right" Margin="0,0,250,0" VerticalAlignment="Top" Style="{DynamicResource Header}" Padding="5" HorizontalContentAlignment="Center" />
        <Label Content="v" HorizontalAlignment="Center" Margin="0,10,0,0" VerticalAlignment="Top" Style="{DynamicResource TextLabel}" HorizontalContentAlignment="Center" />
    </Grid>
</UserControl>