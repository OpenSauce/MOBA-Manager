<Window x:Class="MOBA_Manager.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MOBA_Manager"
        mc:Ignorable="d"
        Title="MobA Manager" WindowStartupLocation="CenterScreen" WindowState="Maximized" Background="Black">
    <Window.Effect>
        <DropShadowEffect />
    </Window.Effect>
    <Grid>
        <Frame x:Name="MainWindowFrame" Content="Frame" Background="Black" />
    </Grid>
</Window>